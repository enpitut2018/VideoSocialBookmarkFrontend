{
  "name": "video_social_bookmark_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "immutability-helper": "^2.8.1",
    "json-server": "^0.14.0",
    "query-string": "^6.2.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-media": "^1.8.0",
    "react-paginate": "^6.0.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-share": "^2.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "redux-token-auth": "amgad-naiem/redux-token-auth#master",
    "serve": "^10.0.2",
    "styled-components": "^4.0.3"
  },
  "scripts": {
    "start:dev": "PORT=3001 react-scripts start",
    "start:prod": "yarn build && cp -r build/* ../public",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "yarn eslint --fix src/actions src/components src/config src/reducers src/*.js",
    "style-lint": "yarn stylelint src/components/**/*.js",
    "lint": "yarn format && yarn style-lint",
    "mock": "yarn json-server api.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.2",
    "stylelint": "^9.8.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/(*.js|(actions|components|reducers|config)/**/*.js)": [
      "yarn eslint --fix",
      "git add"
    ],
    "src/components/**/*.js": [
      "yarn stylelint",
      "git add"
    ]
  }
}
